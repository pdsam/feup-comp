import static io.println(int);

class If {

    public static void main(String[] args){
        If varIf;
        int a;
        int b;

        varIf = new If();
        a = 0;
        b = 2;

        a = varIf.cond(a);
        b = varIf.isGreater(a, b);

        io.println(a);

    }

    public boolean cond(int val) {
        return (val < 0);
    }

    public boolean isGreater(int val, int val1) {
        return (val < val1);
    }

}