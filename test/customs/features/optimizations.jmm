
import static io.print(int) void;

class RegisterAllocation {

    public static void main(String[] args){
        RegisterAllocation alocator;
        alocator = new RegisterAllocation();
        alocator.func();
    }

     public int func(){
            int size;
            int i;
            i = 0;
            size = 20;
            while( i < size) {//we know 'size' will not change event inside the while stmt, but 'i' changes!
           	io.print(i);
           	i=i+1;
            }
            return 0;
     }

}